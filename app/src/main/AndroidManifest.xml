<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="th.qsds.app.tamis.in.th.qsdscenter">

  <uses-permission android:name="android.permission.INTERNET"/>

  <application
      android:name="th.or.nectec.app.qsdscenter.LauncherApplication"
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:theme="@style/FullscreenTheme">

    <activity
        android:name="th.or.nectec.app.qsdscenter.LauncherActivity"
        android:configChanges="orientation|keyboardHidden|screenSize"
        android:label="@string/app_name">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>
        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>

    <activity
        android:name="th.or.nectec.app.qsdscenter.WebViewActivity">
    </activity>
  </application>

</manifest>
